config ech-wk 'general'
	option enabled '0'
	option auto_start '1'
	option listen_addr '127.0.0.1:30000'
	option server_ip ''
	option dns_server 'dns.alidns.com/dns-query'
	option ech_domain 'cloudflare-ech.com'
	option proxy_mode 'bypass_cn'

config server
	option name '默认服务器'
	option server_addr ''
	option token ''
	option active '1'
